<resources>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ProgressCardView">
        <attr name="dataType">
            <enum name="YEAR" value="3" />
            <enum name="MONTH" value="2" />
            <enum name="WEEK" value="1" />
            <enum name="DAY" value="0" />
        </attr>
    </declare-styleable>
    <declare-styleable name="DayNightLightProgressView">
        <attr name="day_light" format="boolean" />
    </declare-styleable>
</resources>