<resources>

    <style name="Widget.YearlyProgress.AppWidget.Container" parent="android:Widget">
        <item name="android:id">@android:id/background</item>
        <item name="android:padding">?attr/appWidgetPadding</item>
    </style>

    <style name="Widget.YearlyProgress.AppWidget.InnerView" parent="android:Widget">
        <item name="android:padding">?attr/appWidgetPadding</item>
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>

    <style name="WidgetProgressBarBase" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:indeterminate">false</item>
        <item name="android:max">100</item>
        <item name="android:progressDrawable">@drawable/rounded_progress_bar</item>
    </style>

    <style name="CircularProgressIndicatorBase" parent="android:Widget.ProgressBar">
        <item name="android:padding">5dp</item>
        <item name="android:progressDrawable">@drawable/circular_progress_drawable</item>
        <item name="android:max">100</item>
    </style>

    <!-- Progress Bar Themes -->

